class Solution {
public:
    vector<string> divideString(string s, int k, char fill) {
        vector<string> result;

        for(int i = 0; i < s.length(); i+=k){
            string g = s.substr(i, k);

            while(g.length() < k){
                g+= fill;
            }
            result.push_back(g);
        }
        return result;
    }
};
